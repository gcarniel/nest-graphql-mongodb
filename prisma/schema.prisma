// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Books {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  name String

  description String

  authorName String

  @@map("books")
}
